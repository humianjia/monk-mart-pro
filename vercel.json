{
  "version": 2,
  "builds": [
    {
      "src": "**",
      "use": "@vercel/static"
    }
  ],
  "rewrites": [
    { "source": "/", "destination": "/game1.html" },
    { "source": "/guide", "destination": "/guide.html" },
    { "source": "/advanced", "destination": "/advanced.html" },
    { "source": "/blog", "destination": "/blog.html" },
    { "source": "/forum", "destination": "/forum.html" }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}
